{
  "manifest_version": 2,

  "name": "NewTab.Party",
  "description": "See something interesting every time you click New Tab",
  "version": "0.0.3",

  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "suggest.html"
  },
  "chrome_url_overrides" : {
    "newtab": "view.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://www.newtab.com/*",
    "https://www.newtab.com/*",
    "https://api.github.com/*",
    "https://cdn.rawgit.com/*",
    "https://api.flickr.com/*",
    "https://*.staticflickr.com/*"
  ]
}
